<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDialog?>
<?import com.jfoenix.controls.JFXDialogLayout?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="stackpane" stylesheets="@../../css/dialog.css"
           maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
           xmlns="http://javafx.com/javafx/10.0.2-internal"
           fx:controller="ph.parcs.rmhometiles.ui.UserNotFoundDialogController">

    <JFXDialog fx:id="warningDialog" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">

        <JFXDialogLayout alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <heading>
                <VBox alignment="CENTER">
                    <FontAwesomeIconView size="3em" styleClass="warning-icon" textAlignment="CENTER"/>
                </VBox>
            </heading>

            <body>
                <Label fx:id="lblContent" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308" textAlignment="CENTER">User not found.
                </Label>
            </body>
            <JFXButton onAction="#close" alignment="CENTER" contentDisplay="CENTER"
                       maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="button-remove"
                       text="Dismiss" textAlignment="CENTER"/>
        </JFXDialogLayout>
    </JFXDialog>

</StackPane>
