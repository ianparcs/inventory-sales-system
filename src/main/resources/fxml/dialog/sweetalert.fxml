<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDialog?>
<?import com.jfoenix.controls.JFXDialogLayout?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import de.jensd.fx.glyphs.GlyphsStack?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<JFXDialog xmlns:fx="http://javafx.com/fxml/1"
           xmlns="http://javafx.com/javafx/10.0.2-internal"
           fx:id="dialog" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
           stylesheets="@../../css/sweetalert/sweetalert.css" StackPane.alignment="CENTER">

    <JFXDialogLayout alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
        <heading>
            <GlyphsStack>
                <FontAwesomeIconView size="6em" styleClass="circle-icon"/>
                <FontAwesomeIconView size="3em" styleClass="trash-icon"/>
            </GlyphsStack>

        </heading>

        <body>
            <Label fx:id="lblMessage" alignment="CENTER" contentDisplay="CENTER"
                   maxHeight="1.7976931348623157E308"
                   maxWidth="1.7976931348623157E308" textAlignment="CENTER"/>
        </body>
        <actions>
            <JFXButton alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308" prefWidth="220.0" styleClass="button-cancel" text="Cancel"
                       textAlignment="CENTER"/>
            <JFXButton fx:id="btnRemove" textAlignment="CENTER"
                       alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308" prefWidth="220.0" styleClass="button-remove" text="Remove"/>
        </actions>
    </JFXDialogLayout>
</JFXDialog>


